#!/usr/bin/env zsh
# Defines environment variables.
#

# Ensure path arrays do not contain duplicates.
typeset -gU cdpath fpath path mailpath

export HOMEBREW_GITHUB_API_TOKEN=a00425e10b96195220998287b9cede51a890aa14
export HOMEBREW_NO_ANALYTICS=1
export NODE_TLS_REJECT_UNAUTHORIZED=0
export HOMEBREW_NO_INSECURE_REDIRECT=1
export TRAVIS_GIT_TOKEN=be8c34db234f52973e3651495b1fdabf3df21da3
setopt extended_glob
# export HOMEBREW_CASK_OPTS=--require-sha

# Ensure that a non-login, non-interactive shell has a defined environment.
if [[ "$SHLVL" -eq 1 && ! -o LOGIN && -s "${ZDOTDIR:-$HOME}/.zprofile" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprofile"
fi

if [[ ! -d "$TMPDIR" ]]; then
  export TMPDIR="/tmp/$LOGNAME"
  mkdir -p -m 700 "$TMPDIR"
fi

TMPPREFIX="${TMPDIR%/}/zsh"

# Set the list of directories that Zsh searches for programs.
path=(
"$HOME/bin"
"$HOME/.gem/ruby/2.0.0/bin"
/usr/local/opt/openssl/bin
/usr/{local/,}{bin,sbin}
/bin
"$path[@]"
)
#/Users/ryan/.zprezto/modules/**/*(/)
#)
